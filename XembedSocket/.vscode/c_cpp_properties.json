{
    "configurations": [
        {
            "name": "General Host / X86-Ubuntu1804 Target",
            "compilerPath": "/opt/clang-15/bin/clang++",
            "compilerArgs": [
                "--sysroot=/opt/xtools-uni/x86-ubuntu1804",
                "--target=x86_64-ubuntu18.04-linux-gnu"
            ],
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}/../include"
            ],
            "defines": [
                "SPADAS_ENV_LINUX",
                "SPADAS_ARCH_X86"
            ],
            "intelliSenseMode": "clang-x64",
            "cppStandard": "c++11"
        },
        {
            "name": "General Host / Arm-Ubuntu1604 Target",
            "compilerPath": "/opt/clang-15/bin/clang++",
            "compilerArgs": [
                "--sysroot=/opt/xtools-uni/arm-ubuntu1604",
                "--target=aarch64-ubuntu16.04-linux-gnu"
            ],
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}/../include"
            ],
            "defines": [
                "SPADAS_ENV_LINUX",
                "SPADAS_ARCH_ARM"
            ],
            "intelliSenseMode": "clang-arm64",
            "cppStandard": "c++11"
        }
    ],
    "version": 4
}