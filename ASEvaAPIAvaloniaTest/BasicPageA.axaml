<Panel xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        x:Class="ASEvaAPIAvaloniaTest.BasicPageA"
        mc:Ignorable="d" d:DesignWidth="600" d:DesignHeight="300"
        Background="LightYellow">
    <Grid RowDefinitions="Auto,8,Auto">
        <Label Grid.Row="0" Content="{DynamicResource basic-menu-notice}" />
    </Grid>
    <Control.ContextMenu>
        <ContextMenu>
            <MenuItem Header="{DynamicResource menu-button}" Click="itemMenu_Click">
                <MenuItem.Icon>
                    <Image Source="resm:menu-button.png?assembly=ASEvaAPIAvaloniaTest" />
                </MenuItem.Icon>
            </MenuItem>
        </ContextMenu>
    </Control.ContextMenu>
</Panel>