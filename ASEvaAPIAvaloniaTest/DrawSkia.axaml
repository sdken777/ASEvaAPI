<Panel xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ASEvaAPIAvaloniaTest"
        xmlns:aseva="clr-namespace:ASEva.UIAvalonia;assembly=ASEvaAPIAvalonia"
        x:Class="ASEvaAPIAvaloniaTest.DrawSkia"
        mc:Ignorable="d" d:DesignWidth="200" d:DesignHeight="300" Width="200">
    <Border CornerRadius="{StaticResource GlobalCornerRadius}" Background="LightYellow">
        <aseva:SkiaView x:Name="skiaView" />
    </Border>

    <Panel.Resources>
        <ResourceDictionary>
            <ResourceDictionary x:Key="en">
                <x:String x:Key="draw-text">Hello World</x:String>
                <x:String x:Key="draw-skia-anti-alias">Anti-alias Test</x:String>
            </ResourceDictionary>
            <ResourceDictionary x:Key="zh">
                <x:String x:Key="draw-text">天气不错</x:String>
                <x:String x:Key="draw-skia-anti-alias">反锯齿测试</x:String>
            </ResourceDictionary>
        </ResourceDictionary>
    </Panel.Resources>
</Panel>