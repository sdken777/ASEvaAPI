<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        x:Class="ASEvaAPIAvaloniaTest.MainWindow"
        Title="Test"
        Width="400" Height="200" MinWidth="400" MinHeight="200">
    <StackPanel Margin="8">
        <CheckBox x:Name="checkChineseLanguage" Content="Chinese" />
        <CheckBox x:Name="checkOnscreenRendering" Content="Onscreen Rendering" />
        <Button x:Name="buttonShowEtoWindow" Content="Show Eto Window" Click="buttonShowEtoWindow_Click" />
        <Button x:Name="buttonShowEtoDialog" Content="Show Eto Dialog" Click="buttonShowEtoDialog_Click" />
        <Button x:Name="buttonShowEtoEmbedWindow" Content="Show Eto Embed Window" Click="buttonShowEtoEmbedWindow_Click" />
        
    </StackPanel>
</Window>
