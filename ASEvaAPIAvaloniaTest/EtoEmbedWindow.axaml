<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:aseva="clr-namespace:ASEva.UIAvalonia;assembly=ASEvaAPIAvalonia"
        x:Class="ASEvaAPIAvaloniaTest.EtoEmbedWindow"
        Title="{DynamicResource eto-window}" Icon="resm:icon.png" 
        Width="1350" Height="800" MinWidth="1350" MinHeight="800">
    <Grid x:Name="grid">
        <aseva:EtoEmbedder x:Name="embedder" />
    </Grid>

    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary x:Key="en">
                <x:String x:Key="eto-window">Eto Window</x:String>
            </ResourceDictionary>
            <ResourceDictionary x:Key="zh">
                <x:String x:Key="eto-window">Eto窗口</x:String>
            </ResourceDictionary>
        </ResourceDictionary>
    </Window.Resources>
</Window>
