{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "process",
            "label": "(internal)清理",
            "command": "rm",
            "args": [
                "-rf",
                "./obj",
                "./bin"
            ],
            "problemMatcher": []
        },
        {
            "type": "process",
            "label": "(internal)发布构建(X86-Windows目标)",
            "command": "dotnet",
            "args": [
                "build",
                "./ASEvaAPIEto.csproj",
                "-c",
                "Release",
                "-o",
                "../bin64",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "dependsOn": [
                "(internal)清理"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "type": "process",
            "label": "(internal)移除发布构建冗余文件(X86-Windows目标)",
            "command": "rm",
            "args": [
                "-vf",
                "../bin64/ASEvaAPIEto.pdb",
                "../bin64/ASEvaAPIEto.deps.json"
            ],
            "problemMatcher": []
        },
        {
            "type": "process",
            "label": "(internal)创建bin文件夹",
            "command": "mkdir",
            "args": [
                "-p",
                "../binx",
                "../binxa",
                "../binm",
            ],
            "problemMatcher": []
        },
        {
            "type": "process",
            "label": "(internal)拷贝dll等至bin(X86-Windows至X86-Ubuntu1804目标)",
            "command": "cp",
            "args": [
                "-vf",
                "../bin64/ASEvaAPIEto.dll",
                "../bin64/ASEvaAPIEto.xml",
                "../binx/"
            ],
            "problemMatcher": []
        },
        {
            "type": "process",
            "label": "(internal)拷贝dll等至bin(X86-Windows至Arm-Ubuntu1604目标)",
            "command": "cp",
            "args": [
                "-vf",
                "../bin64/ASEvaAPIEto.dll",
                "../bin64/ASEvaAPIEto.xml",
                "../binxa/"
            ],
            "problemMatcher": []
        },
        {
            "type": "process",
            "label": "(internal)拷贝dll等至bin(X86-Windows至X86-MacOS1010目标)",
            "command": "cp",
            "args": [
                "-vf",
                "../bin64/ASEvaAPIEto.dll",
                "../bin64/ASEvaAPIEto.xml",
                "../binm/"
            ],
            "problemMatcher": []
        },
        {
            "type": "process",
            "label": "发布构建(全平台目标)",
            "command": "echo",
            "args": [
                "Done.",
            ],
            "dependsOrder": "sequence",
            "dependsOn": [
                "(internal)发布构建(X86-Windows目标)",
                "(internal)移除发布构建冗余文件(X86-Windows目标)",
                "(internal)创建bin文件夹",
                "(internal)拷贝dll等至bin(X86-Windows至X86-Ubuntu1804目标)",
                "(internal)拷贝dll等至bin(X86-Windows至Arm-Ubuntu1604目标)",
                "(internal)拷贝dll等至bin(X86-Windows至X86-MacOS1010目标)",
            ],
            "problemMatcher": []
        },
        {
            "type": "process",
            "label": "调试构建(X86-Windows目标)",
            "command": "dotnet",
            "args": [
                "build",
                "./ASEvaAPIEto.csproj",
                "-o",
                "../bin64/debug",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "dependsOn": [
                "(internal)清理"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "type": "process",
            "label": "调试构建(X86-Ubuntu1804目标)",
            "command": "dotnet",
            "args": [
                "build",
                "./ASEvaAPIEto.csproj",
                "-o",
                "../binx/debug",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "dependsOn": [
                "(internal)清理"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "type": "process",
            "label": "调试构建(Arm-Ubuntu1604目标)",
            "command": "dotnet",
            "args": [
                "build",
                "./ASEvaAPIEto.csproj",
                "-o",
                "../binxa/debug",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "dependsOn": [
                "(internal)清理"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "type": "process",
            "label": "调试构建(X86-MacOS1010目标)",
            "command": "dotnet",
            "args": [
                "build",
                "./ASEvaAPIEto.csproj",
                "-o",
                "../binm/debug",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "dependsOn": [
                "(internal)清理"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "type": "process",
            "label": "调试构建并生成app包(X86-MacOS主机/X86-MacOS1010目标)",
            "command": "../binm/app-maker",
            "args": [
                "${workspaceFolder}/../binm/debug",
                "${workspaceFolder}/../ASEvaAPIEtoTest/Resources/icon.icns",
                "${workspaceFolder}/../binm/debug/ASEvaAPIEtoTest.app",
                "ASEvaAPIEtoTest",
                "icon.icns",
            ],
            "dependsOn": [
                "调试构建(X86-MacOS1010目标)"
            ],
            "problemMatcher": []
        },
    ]
}
