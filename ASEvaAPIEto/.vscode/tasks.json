{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "原始发布构建",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/ASEvaAPIEtoLinux.csproj",
                "-c",
                "Release",
                "-o",
                "${workspaceFolder}/../binx",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "发布文件整理",
            "command": "rm",
            "type": "shell",
            "args": [
                "-vf",
                "${workspaceFolder}/../binx/ASEvaAPIEto.pdb",
                "${workspaceFolder}/../binx/ASEvaAPIEto.deps.json"
            ],
            "problemMatcher": []
        },
        {
            "label": "发布构建",
            "command": "cp",
            "type": "shell",
            "args": [
                "-vf",
                "${workspaceFolder}/../binx/ASEvaAPIEto.*",
                "${workspaceFolder}/../bin64/"
            ],
            "dependsOrder": "sequence",
            "dependsOn": [
                "原始发布构建",
                "发布文件整理",
            ],
            "problemMatcher": []
        },
    ]
}
